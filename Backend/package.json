{
  "name": "intellihealth-backend",
  "version": "1.0.0",
  "description": "IntelliHealth Healthcare Platform Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch --testTimeout=10000",
    "test:coverage": "jest --coverage --testTimeout=10000",
    "seed": "node scripts/seedDatabase.js",
    "seed:sample": "node scripts/pushSampleAccounts.js",
    "migrate": "node scripts/migrateDatabase.js",
    "migrate:rollback": "node scripts/migrateDatabase.js rollback",
    "backup": "node scripts/backupDatabase.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prettier": "prettier --write \"**/*.js\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.40.0",
    "nodemailer": "^6.9.4",
    "moment": "^2.29.4",
    "cron": "^2.4.4",
    "socket.io": "^4.7.2",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "uuid": "^9.0.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-fileupload": "^1.4.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!tests/**"
    ]
  },
  "keywords": [
    "healthcare",
    "medical",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "rest-api"
  ],
  "author": "IntelliHealth Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }}